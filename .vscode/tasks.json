{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc-11 arquivo de build ativo",
      "command": "/usr/bin/gcc-11",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        // "${fileDirname}/**.c",
        // "${fileDirname}/**.h",
        // "${file}",
        "main.c",
        "cards.c",
        "debugger.c",
        "game.c",
        "hand.c",
        "strategy.c",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Tarefa gerada pelo Depurador."
    }
  ],
  "version": "2.0.0"
}